pipeline {
    agent any 
    options {
        disableConcurrentBuilds()
        timestamps()
        buildDiscarder(logRotator(numTokeepstr: '10'))
    }

    stages {
        stage('jobs') {
           steps {
                
                sh """
                   job2.groovy
                   maven.groovy
                   job3.groovy
               """
           }
            
        }
    }


}
